<?ifndef TestName?>
<?define TestName=TransformTests?>
<?endif?>

<?ifndef Version?>
<?define Version=1.0.0.0?>
<?endif?>

<Wix xmlns="http://wixtoolset.org/schemas/v4/wxs">
    <Patch AllowRemoval="yes" Classification="Update" ClientPatchId="PatchB" Description="This is a test patch" DisplayName="$(var.TestName) - PatchB" Manufacturer="WiX Toolset" MinorUpdateTargetRTM="yes">
        <Media Id="100" Cabinet="B" EmbedCab="yes">
            <PatchBaseline Id="PatchB" />
        </Media>

        <PatchFamily Id="A" Version="$(var.Version)" Supersede="yes">
          <ComponentRef Id="ComponentA" />
        </PatchFamily>
        <PatchFamily Id="B" Version="$(var.Version)" Supersede="yes">
          <ComponentRef Id="ComponentB" />
        </PatchFamily>
    </Patch>
</Wix>
